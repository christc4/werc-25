body,footer,header nav,nav ul{display:flex}header h1,header nav{color:#000;border-bottom:2px solid #000}footer a,header a,nav a{color:inherit}body,body>nav,table{padding:0}.up-button,a,body>nav>div a:hover,header a,header a:hover,nav a{text-decoration:none}figcaption,header h1 span{font-style:italic}body{flex-wrap:wrap;background-color:#ffffea;margin:0;font-size:84%;}footer,header{flex-basis:100%;flex-shrink:0}article{flex-basis:60%;padding:.5ex 0 5vh 1vw}header nav{justify-content:space-between;background-color:#eaffff;padding:.3em;font-size:91%}header h1 span{margin-left:1em;font-size:50%}body>nav{flex-basis:content;min-width:16em}nav ul{flex-direction:column;list-style-type:none;list-style-position:outside;padding-left:0}nav li ul{padding-left:.6em}footer{justify-content:space-between;color:#000;border-top:1px solid #000}a:hover{text-decoration:underline}.thisPage{color:#000}header h1{background-color:#ff6d06;margin:0;font-weight:400;padding:.25ex;font-size:233%}header>nav>div a:hover{background-color:#9eeeee;color:#000}body>nav>div a:hover,footer>div a:hover{color:#000;background-color:#eeee9e}body>nav{border-right:1px solid #000}body>nav>div{border-bottom:1px solid #000}body>nav>div a{color:#99994c;display:block;text-transform:capitalize;font-weight:700;padding:.25em 1ex .25em 2mm;font-size:102%}body>nav>div a:hover{border-left:.2em solid #000}body>nav>div p{font-weight:700;margin:0 0 .5em 2mm;padding:1em 0 0}article h1,article h2{color:#060;font-weight:700;margin:2em 0 0;border-bottom:2px solid #060}article h3,article h4,article h5,article h6,article h7,article h8{color:#060;font-weight:700;margin:2em 0 0}article a{color:#06c}article a:hover{color:#6487dc}article pre{font-size:1.2em}footer div{padding:1em}table{border:1px solid rgba(128,128,128,.5)}th{color:#fff;background-color:#6487dc}tr:nth-child(odd){background-color:rgba(128,128,128,.1)}.up-button{position:fixed;top:80%;left:11%;transform:translate(-50%,-50%);height:30px;width:150px;z-index:2;background-color:rgba(136,136,204,.4);color:rgba(0,0,0,.5);cursor:pointer;text-overflow:ellipsis;overflow:hidden;white-space:nowrap;font-size:12px;line-height:30px}
